{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": ".NET: EventHub Sender",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-sender",
            "program": "${workspaceFolder}/examples/dotnet/EventHubSender/bin/Debug/net8.0/EventHubSender.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/dotnet/EventHubSender",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "env": {
                "EVENT_HUB_FULLY_QUALIFIED_NAMESPACE": "${env:EVENT_HUB_FULLY_QUALIFIED_NAMESPACE}",
                "EVENT_HUB_NAME": "${env:EVENT_HUB_NAME}"
            }
        },
        {
            "name": ".NET: EventHub Receiver",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-receiver",
            "program": "${workspaceFolder}/examples/dotnet/EventHubReceiver/bin/Debug/net8.0/EventHubReceiver.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/dotnet/EventHubReceiver",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "env": {
                "EVENT_HUB_FULLY_QUALIFIED_NAMESPACE": "${env:EVENT_HUB_FULLY_QUALIFIED_NAMESPACE}",
                "EVENT_HUB_NAME": "${env:EVENT_HUB_NAME}",
                "STORAGE_ACCOUNT_NAME": "${env:STORAGE_ACCOUNT_NAME}",
                "BLOB_CONTAINER_NAME": "${env:BLOB_CONTAINER_NAME}"
            }
        }
    ]
}